{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug with CMake",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/bin/super_trunfo",
        "cwd": "${workspaceFolder}",
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ]
  }
  